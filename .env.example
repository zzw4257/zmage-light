# Zmage 环境变量配置
# 复制此文件为 .env 并填入实际值

# Gemini API (必需)
GEMINI_API_KEY=your_gemini_api_key_here

# 端口配置
WEB_PORT=2333
API_PORT=4257

# PostgreSQL 数据库
DATABASE_URL=postgresql://zmage:zmage_password@postgres:5432/zmage
POSTGRES_USER=zmage
POSTGRES_PASSWORD=zmage_password
POSTGRES_DB=zmage

# Redis
REDIS_URL=redis://redis:6379/0

# Qdrant 向量数据库
QDRANT_URL=http://qdrant:6333

# MinIO 对象存储
S3_ENDPOINT=http://minio:9000
S3_ACCESS_KEY=minioadmin
S3_SECRET_KEY=minioadmin
S3_BUCKET=zmage

# JWT 密钥 (用于用户认证)
JWT_SECRET=your_jwt_secret_key_change_in_production

# 应用配置
APP_NAME=Zmage
APP_ENV=development
